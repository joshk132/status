            <div class="timeline-panel">
             <div class="timeline-heading">
              <h4 class="timeline-title"><%= status.type %></h4>
              <p><small class="text-muted">11 hours ago</small></p>
             </div>
             <div class="timeline-body">
              <p><%= status.message %></p>
             </div>
            </div>
           </li>
           <% } else if (status.type == 'Outage' && Math.abs(ctr % 2) == 1){ %>
