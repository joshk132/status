<% include ./partials/header %> 

 <div class="container">
     <header class="jumbotron">
         <div class="container">
             <h1>Welcome To Status Node!</h1>
         </div>
     </header>
     <% var ctr = 0 %>
      <div class="container">
      <% statuses.forEach(function(status){ %>
      <% ctr ++ %>
    
         <ul class="timeline">
          <% if (status.type == 'Successful Solution' && ctr % 2 == 0) { %>
          <li>
           <div class="timeline-badge"><i class="glyphicon glyphicon-check"></i></div>
            <div class="timeline-panel">
             <div class="timeline-heading">
              <h4 class="timeline-title"><%= status.type %></h4>
              <p><small class="text-muted">11 hours ago</small></p>
             </div>
             <div class="timeline-body">
              <p><%= status.message %></p>             
             </div>
            </div>
           </li>
           <% } else if (status.type == 'Successful Solution' && Math.abs(ctr % 2) == 1){ %>
           <li class="timeline-inverted">
            <div class="timeline-badge warning"><i class="glyphicon glyphicon-check"></i></div>
             <div class="timeline-panel">
              <div class="timeline-heading">
               <h4 class="timeline-title"><%= status.type %></h4>
               <p><small class="text-muted">11 hours ago</small></p>
              </div>
              <div class="timeline-body">
               <p><%= status.message %></p>
              </div>
            </div>
           </li>
           <% } %>
           
           <% if (status.type == 'Planned Maintenance' && ctr % 2 == 0) { %>
          <li>
           <div class="timeline-badge"><i class="glyphicon glyphicon-info-sign"></i></div>
            <div class="timeline-panel">
             <div class="timeline-heading">
              <h4 class="timeline-title"><%= status.type %></h4>
              <p><small class="text-muted">11 hours ago</small></p>
             </div>
             <div class="timeline-body">
              <p><%= status.message %></p>             
             </div>
            </div>
           </li>
           <% } else if (status.type == 'Planned Maintenance' && Math.abs(ctr % 2) == 1){ %>
           <li class="timeline-inverted">
            <div class="timeline-badge warning"><i class="glyphicon glyphicon-info-sign"></i></div>
             <div class="timeline-panel">
              <div class="timeline-heading">
               <h4 class="timeline-title"><%= status.type %></h4>
               <p><small class="text-muted">11 hours ago</small></p>
              </div>
              <div class="timeline-body">
               <p><%= status.message %></p>
              </div>
            </div>
           </li>
           <% } %>
           
           <% if (status.type == 'Unplanned Maintenance' && ctr % 2 == 0) { %>
          <li>
           <div class="timeline-badge"><i class="glyphicon glyphicon-alert"></i></div>
            <div class="timeline-panel">
             <div class="timeline-heading">
              <h4 class="timeline-title"><%= status.type %></h4>
              <p><small class="text-muted">11 hours ago</small></p>
             </div>
             <div class="timeline-body">
              <p><%= status.message %></p>             
             </div>
            </div>
           </li>
           <% } else if (status.type == 'Unplanned Maintenance' && Math.abs(ctr % 2) == 1){ %>
           <li class="timeline-inverted">
            <div class="timeline-badge warning"><i class="glyphicon glyphicon-alert"></i></div>
             <div class="timeline-panel">
              <div class="timeline-heading">
               <h4 class="timeline-title"><%= status.type %></h4>
               <p><small class="text-muted">11 hours ago</small></p>
              </div>
              <div class="timeline-body">
               <p><%= status.message %></p>
              </div>
            </div>
           </li>
           <% } %>
           
           <% if (status.type == 'Incident' && ctr % 2 == 0) { %>
          <li>
           <div class="timeline-badge"><i class="glyphicon glyphicon-option-horizontal"></i></div>
            <div class="timeline-panel">
             <div class="timeline-heading">
              <h4 class="timeline-title"><%= status.type %></h4>
              <p><small class="text-muted">11 hours ago</small></p>
             </div>
             <div class="timeline-body">
              <p><%= status.message %></p>             
             </div>
            </div>
           </li>
           <% } else if (status.type == 'Incident' && Math.abs(ctr % 2) == 1){ %>
           <li class="timeline-inverted">
            <div class="timeline-badge warning"><i class="glyphicon glyphicon-option-horizontal"></i></div>
             <div class="timeline-panel">
              <div class="timeline-heading">
