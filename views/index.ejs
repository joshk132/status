             <div class="timeline-panel">
              <div class="timeline-heading">
               <h4 class="timeline-title"><%= status.type %></h4>
               <p><small class="text-muted">11 hours ago</small></p>
              </div>
              <div class="timeline-body">
               <p><%= status.message %></p>
              </div>
            </div>
           </li>
           <% } %>
           
           <% if (status.type == 'Service Interruption' && ctr % 2 == 0) { %>
          <li>
           <div class="timeline-badge"><i class="glyphicon glyphicon-flag"></i></div>
            <div class="timeline-panel">
             <div class="timeline-heading">
              <h4 class="timeline-title"><%= status.type %></h4>
              <p><small class="text-muted">11 hours ago</small></p>
             </div>
             <div class="timeline-body">
              <p><%= status.message %></p>             
